  "scripts": {
    "ng": "ng",
    "ng-high-memory": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng",
    "start": "npm run build-pre && npm run serve & npm run test",
    "start:live": "npm run build-pre && npm run serve:live & npm run test",
    "serve": "ng serve --proxy-config proxy.conf.json",
    "serve:live": "ng serve --proxy-config proxy.live.conf.json",
    "serve:poll": "ng serve --poll=2000 --host 0.0.0.0 --proxy-config proxy.conf.json",
    "build": "npm run build-pre && npm run ng-high-memory -- build --configuration production --base-href=/ng/ --output-path=../ng",
    "build-pre": "node ./build/build-pre.js",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
