{
  "name": "web-ui",
  "license": "CC-BY-NC-SA-4.0",
  "scripts": {
    "ng": "ng",
    "ng-high-memory": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng",
    "start": "yarn serve",
    "build": "node ./build/build-pre.js && yarn run ng-high-memory -- build --deploy-url='/static/'",
    "test": "ng test --watch=true",
    "lint": "ng lint",
    "build:en": "ng build  --deploy-url='/static/' --configuration=production-en",
    "build:nl": "ng build  --deploy-url='/static/' --configuration=production-nl",
    "i18n": "ng extract-i18n --output-path locale",
    "serve": "ng serve --proxy-config ../proxy.conf.json",
    "serve:en": "ng serve --proxy-config ../proxy.conf.json --configuration=en",
    "serve:nl": "ng serve --proxy-config ../proxy.conf.json --configuration=nl",
    "stop": "lsof -t -i tcp:4200 | xargs kill -9 || echo \"not running\"",
    "test-once": "ng test --watch=false",
    "watch": "ng build --prod --watch"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^13.3.11",
    "@angular/cdk": "^12.2.13",
    "@angular/common": "^13.3.11",
    "@angular/core": "^13.3.11",
    "@angular/forms": "^13.3.11",
    "@angular/material": "^12.2.13",
    "@angular/platform-browser": "^13.3.11",
    "@angular/platform-browser-dynamic": "^13.3.11",
    "@angular/platform-server": "^13.3.11",
    "@angular/router": "^13.3.11",
    "@fortawesome/angular-fontawesome": "^0.10.2",
    "@fortawesome/fontawesome-free": "^6.1.2",
    "@fortawesome/fontawesome-svg-core": "^6.1.2",
    "@fortawesome/free-solid-svg-icons": "^6.1.2",
    "@ng-select/ng-select": "^7.4.0",
    "balloon-css": "^1.2.0",
    "bulma": "^0.9.4",
    "bulma-badge": "^3.0.1",
    "bulma-pageloader": "^2.2.0",
    "core-js": "^3.25.0",
    "fast-xml-parser": "^3.21.1",
    "file-saver": "^2.0.2",
    "jquery": "^3.6.1",
    "jquery-ui": "^1.13.2",
    "jszip": "^3.10.1",
    "lassy-xpath": "^0.12.0",
    "lodash": "^4.17.15",
